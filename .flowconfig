[ignore]
.*/node_modules/@storybook/.*
.*/node_modules/axios/.*
.*/node_modules/babel-core/.*
.*/node_modules/babel-polyfill/.*
.*/node_modules/enzyme-adapter-react-16/.*
.*/node_modules/enzyme-to-json/.*
.*/node_modules/enzyme/.*
.*/node_modules/eslint-config-airbnb/.*
.*/node_modules/eslint-config-react-app/.*
.*/node_modules/flow-bin/.*
.*/node_modules/hoist-non-react-statics/.*
.*/node_modules/jest-styled-components/.*
.*/node_modules/lodash/.*
.*/node_modules/normalize.css/.*
.*/node_modules/plop/.*
.*/node_modules/react-dom/.*
.*/node_modules/react-redux/.*
.*/node_modules/react-resize-detector/.*
.*/node_modules/react-router-dom/.*
.*/node_modules/react-scripts/.*
.*/node_modules/react-test-renderer/.*
.*/node_modules/react/.*
.*/node_modules/redux-saga/.*
.*/node_modules/redux/.*
.*/node_modules/reselect/.*
.*/node_modules/styled-tools/.*
.*/node_modules/formik/.*
.*/node_modules/yup/.*

[include]
.*/src/.*

[libs]
flow-typed

[options]
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.file_ext=.css
module.name_mapper='(warning)' -> 'empty/object'
module.name_mapper='(bem-cn)' -> 'empty/object'
module.name_mapper='(autobind-decorator)' -> 'empty/object'
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
esproposal.export_star_as=enable
